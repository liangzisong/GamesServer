package com.mangni.vegaplan.datatable;

public class VipPackageData {
	private String id,jewelcharacter,chipcolor,itemid;
	private int gold=0,jewelnum=0,chipnum=0,itemnum=0,needstone=0;
	public String getId() {
		return id;
	}
	public void setId(String id) {
		this.id = id;
	}
	public int getGold() {
		return gold;
	}
	public void setGold(int gold) {
		this.gold = gold;
	}
	public String getJewelcharacter() {
		return jewelcharacter;
	}
	public void setJewelcharacter(String jewelcharacter) {
		this.jewelcharacter = jewelcharacter;
	}
	public String getChipcolor() {
		return chipcolor;
	}
	public void setChipcolor(String chipcolor) {
		this.chipcolor = chipcolor;
	}
	public String getItemid() {
		return itemid;
	}
	public void setItemid(String itemid) {
		this.itemid = itemid;
	}
	public int getJewelnum() {
		return jewelnum;
	}
	public void setJewelnum(int jewelnum) {
		this.jewelnum = jewelnum;
	}
	public int getChipnum() {
		return chipnum;
	}
	public void setChipnum(int chipnum) {
		this.chipnum = chipnum;
	}
	public int getItemnum() {
		return itemnum;
	}
	public void setItemnum(int itemnum) {
		this.itemnum = itemnum;
	}
	public int getNeedstone() {
		return needstone;
	}
	public void setNeedstone(int needstone) {
		this.needstone = needstone;
	}
}
