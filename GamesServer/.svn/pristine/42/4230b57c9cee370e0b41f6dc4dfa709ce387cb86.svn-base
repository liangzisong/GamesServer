package com.mangni.vegaplan.datatable;

public class RECShopData {
	private String id;
	private int stone=0;
	private double needrmb=0;
	public String getId()
	{
		return id;
	}
	public void setId(String id)
	{
		this.id=id;
	}
	
	public double getNeedrmb()
	{
		return needrmb;
	}
	public void setNeedrmb(double needrmb)
	{
		this.needrmb=needrmb;
	}
	
	public int getStone()
	{
		return stone;
	}
	public void setStone(int stone)
	{
		this.stone=stone;
	}
	
	
}
